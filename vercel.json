{
  "version": 2,
  "builds": [
    { "src": "api/receive.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/receive-data", "dest": "/api/receive.js" },
    { "src": "/api/get-players", "dest": "/api/receive.js" },
    { "src": "/api/get-items", "dest": "/api/receive.js" },
    { "src": "/api/save-lua-code", "dest": "/api/receive.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
